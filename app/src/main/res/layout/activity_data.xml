<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.edwards2james.transportationanalysis_app.data"
    android:background="#ff000000">


    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="@string/back"
        android:id="@+id/button4"
        android:onClick="onBackClick"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="10dp"
        android:background="@drawable/road"
        android:id="@+id/imageView"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="10dp"
        android:background="@drawable/road"
        android:id="@+id/imageView2"
        android:layout_above="@+id/button4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/cost_query"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:textIsSelectable="true"
        android:layout_alignTop="@+id/textView10"
        android:layout_toRightOf="@+id/textView10"
        android:layout_toEndOf="@+id/textView10" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/permile_query"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:textIsSelectable="true"
        android:layout_alignTop="@+id/textView12"
        android:layout_toRightOf="@+id/textView12"
        android:layout_toEndOf="@+id/textView12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mile_query"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:textIsSelectable="true"

        android:layout_alignBottom="@+id/textView11"
        android:layout_toRightOf="@+id/textView11"
        android:layout_toEndOf="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select Mode To Get Data"
        android:id="@+id/textView2"
        android:textColor="#fffff000"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AIRPLANE"
        android:id="@+id/button6"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button28"
        android:layout_alignEnd="@+id/button28"
        android:onClick="onAirDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BOAT/FERRY"
        android:id="@+id/button26"
        android:layout_alignBottom="@+id/button6"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:nestedScrollingEnabled="false"
        android:onClick="onBoatDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BIKE"
        android:id="@+id/button27"
        android:layout_above="@+id/button29"
        android:layout_alignLeft="@+id/button32"
        android:layout_alignStart="@+id/button32"
        android:layout_toLeftOf="@+id/button26"
        android:layout_toStartOf="@+id/button26"
        android:onClick="onBikeDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BUS"
        android:id="@+id/button28"
        android:layout_below="@+id/button6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button30"
        android:layout_alignEnd="@+id/button30"
        android:onClick="onBusDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CAR"
        android:id="@+id/button29"
        android:layout_alignTop="@+id/button28"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/button31"
        android:layout_alignStart="@+id/button31"
        android:onClick="onCarDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MOTOCYCLE/SCOOTER"
        android:id="@+id/button30"
        android:layout_below="@+id/button28"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/button4"
        android:layout_toStartOf="@+id/button4"
        android:onClick="onMotoDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WALK"
        android:id="@+id/button31"
        android:layout_below="@+id/button29"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/button26"
        android:layout_alignStart="@+id/button26"
        android:layout_alignBottom="@+id/button32"
        android:onClick="onWalkDataClick" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TRAIN"
        android:id="@+id/button32"
        android:layout_alignBottom="@+id/button30"
        android:layout_toRightOf="@+id/button30"
        android:layout_below="@+id/button28"
        android:layout_toLeftOf="@+id/button29"
        android:layout_toStartOf="@+id/button29"
        android:onClick="onTrainDataClick" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GET DATA"
        android:id="@+id/textView3"
        android:textColor="#fffffa00"
        android:textSize="25dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expenses: $ "
        android:id="@+id/textView10"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Miles: "
        android:id="@+id/textView11"
        android:layout_marginTop="33dp"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cost Per Mile: $ "
        android:id="@+id/textView12"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:layout_marginTop="33dp" />

</RelativeLayout>
